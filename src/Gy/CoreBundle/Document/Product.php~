namespace Gy\CoreBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Product
{
    
    /**
     * @var MongoId $id
     */
    protected $id;

    /**
     * @var string $name
     */
    protected $name;

    /**
     * @var float $price
     */
    protected $price;

    /**
     * @var string $description
     */
    protected $description;

    /**
     * @var collection $category
     */
    protected $category;

    /**
     * @var date $createdAt
     */
    protected $createdAt;

}
